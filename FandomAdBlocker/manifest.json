{
    "manifest_version": 3,
    "name": "FADBlock â€” block ads across the fandoms",
    "version": "2.0.0",
    "description": "Block pop-ups and intrusive ads across Fandom with FADBlock",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
          "matches": ["*://*.fandom.com/*"],
          "js": ["js/website.js"]
        }
      ],
     "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "64": "img/64.png",
        "96": "img/96.png",
        "128": "img/128.png"
     }
}